  "main": "client/src/index.tsx",
  "scripts": {
    "dev": "concurrently \"npm run app\" \"npm run server\"",
    "app": "vite",
    "server": "env-cmd nodemon --watch server --watch graphql --watch prisma server/index.ts",
    "docker-build": "docker-compose -f docker-compose.dev.yml build",
    "up": "docker-compose -f docker-compose.dev.yml up",
    "bin": "ts-node --project tsconfig.node.json bin/index",
    "introspect": "rover graph introspect http://localhost:4000/graphql > graphql/generated/schema.graphql",
    "generate": "graphql-codegen --config codegen.yml",
    "prettier": "npx prettier --write client server bin"
  },
