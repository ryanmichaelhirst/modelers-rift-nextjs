datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  username String
  password String
}

model Champion {
  id     Int    @id @default(autoincrement())
  name   String
  models Model[]
}

model Model {
  id          Int      @id @default(autoincrement())
  champion    Champion @relation(fields: [championId], references: [id])
  championId  Int
  name        String   @unique
  url         String
}