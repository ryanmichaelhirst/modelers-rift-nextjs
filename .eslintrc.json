{
  "root": true,
  "extends": ["prettier", "plugin:markdown/recommended", "plugin:@next/next/recommended"],
  "plugins": ["@typescript-eslint", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "indent": 0,
    "jsx-no-multiline-js": 0,
    "newline-before-return": "error",
    "curly": 0,
    "no-console": 0,
    "react/jsx-no-bind": 0,
    "object-literal-sort-keys": 0,
    "jsx-boolean-value": 0,
    "jsx-alignment": 0,
    "prettier/prettier": ["error"],
    "camelcase": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
