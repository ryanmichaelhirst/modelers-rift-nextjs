{
  "compilerOptions": {
    "skipLibCheck": true,
    "outDir": "./dist/",
    "noImplicitAny": false,
    "lib": ["dom", "es2022"],
    "target": "es5",
    "module": "NodeNext",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": ["."],
  "exclude": ["node_modules", "input", "output"]
}
